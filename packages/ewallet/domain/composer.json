{
    "name": "ewallet/domain",
    "description": "Ewallet core domain",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": "tests/src"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../hexagonal/hexagonal"
        }
    ],
    "require": {
        "beberlei/assert": "^2.7",
        "hexagonal/hexagonal": "^1.0",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^6.0",
        "giorgiosironi/eris": "^0.9",
        "fzaninotto/faker": "^1.6"
    }
}
