{
    "name": "ewallet/web",
    "description": "Ewallet web application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ewallet/application": "1.0",
        "monolog/monolog": "1.25.3",
        "slim/slim": "3.12.3",
        "twig/twig": "2.12.3",
        "vlucas/phpdotenv": "3.6.0"
    },
    "require-dev": {
        "codeception/codeception": "4.0.1",
        "codeception/module-webdriver": "1.0.0",
        "consolidation/robo": "1.4.11",
        "friendsofphp/php-cs-fixer": "2.16.1",
        "infection/infection": "0.13.6",
        "mockery/mockery": "1.3.1",
        "nelmio/alice": "3.5.8",
        "phpstan/phpstan": "0.11.19",
        "phpstan/phpstan-strict-rules": "0.11.1",
        "phpunit/phpunit": "8.5.1",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "7.2.19"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Application\\": "src/",
            "Ewallet\\": "src/",
            "Adapters": "src/",
            "UI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
