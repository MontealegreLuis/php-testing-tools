{
    "name": "ewallet/definitions",
    "description": "Ewallet common dependency injection definitions",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet/doctrine"
        },
        {
            "type": "path",
            "url": "../../ewallet/application"
        },
        {
            "type": "path",
            "url": "../../ewallet/domain"
        },
        {
            "type": "path",
            "url": "../../hexagonal/hexagonal"
        },
        {
            "type": "path",
            "url": "../../ewallet/responder"
        },
        {
            "type": "path",
            "url": "../../ewallet/templating"
        },
        {
            "type": "path",
            "url": "../../ewallet/validation"
        }
    ],
    "require": {
        "pimple/pimple": "^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^5.4",
        "twig/twig": "^1.24",
        "ewallet/doctrine": "^1.0",
        "ewallet/responder": "^1.0",
        "ewallet/validation": "^1.0",
        "monolog/monolog": "^1.20"
    }
}
