{
    "name": "ewallet/domain",
    "description": "Ewallet core domain",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "beberlei/assert": "~3.1",
        "doctrine/annotations": "^1.6",
        "doctrine/cache": "^1.8",
        "doctrine/orm": "^2.6",
        "moneyphp/money": "^3.0",
        "pimple/pimple": "^3.2",
        "symfony/validator": "^4.1"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "fzaninotto/faker": "^1.6",
        "giorgiosironi/eris": "~0.11",
        "jms/serializer": "^1.13",
        "mockery/mockery": "^1.2",
        "nelmio/alice": "^3.5",
        "php-amqplib/php-amqplib": "^2.8",
        "phpspec/phpspec": "~5.1",
        "phpunit/phpunit": "~7.4",
        "roave/security-advisories": "dev-master",
        "vlucas/phpdotenv": "^2.5"
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Ewallet\\": "src/",
            "Application\\": "src/",
            "Ports\\": "src/"
        },
        "files": [
            "annotations.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/src"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    }
}
