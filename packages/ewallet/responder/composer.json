{
    "name": "ewallet/responder",
    "description": "Ewallet Web Responder port.",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "tests/src",
                "vendor/ewallet/doctrine/tests/src",
                "vendor/ewallet/domain/tests/src"
            ]
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../hexagonal/hexagonal"
        },
        {
            "type": "path",
            "url": "../domain"
        },
        {
            "type": "path",
            "url": "../application"
        },
        {
            "type": "path",
            "url": "../doctrine"
        },
        {
            "type": "path",
            "url": "../templating"
        }
    ],
    "require": {
        "comphppuebla/easy-forms": "^1.0",
        "zendframework/zend-diactoros": "^1.1",
        "ewallet/application": "^1.0",
        "ewallet/doctrine": "^1.0",
        "ewallet/templating": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "nelmio/alice": "^2.0"
    }
}
