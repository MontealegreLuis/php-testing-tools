{
    "name": "ewallet/console",
    "description": "Ewallet console application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ewallet/application": "~1.0",
        "symfony/console": "4.2.8",
        "vlucas/phpdotenv": "3.3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.15.0",
        "infection/infection": "0.13.1",
        "nelmio/alice": "3.5.7",
        "phpstan/phpstan": "0.11.8",
        "phpstan/phpstan-strict-rules": "0.11.1",
        "phpunit/phpunit": "8.1.6",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "platform": {
            "php": "7.2.19"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src",
                "tests/integration"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
