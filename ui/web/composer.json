{
    "name": "ewallet/web",
    "description": "Ewallet web application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ewallet/application": "~1.0",
        "monolog/monolog": "~1.24",
        "slim/slim": "~3.11",
        "twig/twig": "~2.5",
        "vlucas/phpdotenv": "~3.3"
    },
    "require-dev": {
        "codeception/codeception": "~3.0@dev",
        "consolidation/robo": "~1.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "infection/infection": "^0.13.1",
        "mockery/mockery": "~1.2",
        "nelmio/alice": "~3.5",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "~8.1",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Application\\": "src/",
            "Ewallet\\": "src/",
            "Adapters": "src/",
            "UI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
