{
    "name": "ewallet/console",
    "description": "Ewallet console application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ewallet/application": "1.0",
        "symfony/console": "5.2.1",
        "symfony/dependency-injection": "5.2.1",
        "vlucas/phpdotenv": "5.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.17.3",
        "infection/infection": "0.20.2",
        "nelmio/alice": "3.7.4",
        "phpstan/phpstan": "0.12.64",
        "phpstan/phpstan-strict-rules": "0.12.7",
        "phpunit/phpunit": "9.4.4",
        "rector/rector": "0.8.56",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "platform": {
            "php": "7.4.12"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src",
                "tests/integration"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
