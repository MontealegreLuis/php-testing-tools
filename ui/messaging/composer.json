{
    "name": "ewallet/messaging",
    "description": "Ewallet messaging",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ewallet/application": "1.0",
        "laminas/laminas-mail": "2.12.5",
        "moneyphp/money": "3.3.1",
        "nesbot/carbon": "2.43.0",
        "psr/container": "1.0.0",
        "symfony/console": "5.2.1",
        "vlucas/phpdotenv": "5.2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "2.13.2",
        "friendsofphp/php-cs-fixer": "2.17.3",
        "fzaninotto/faker": "1.8.0",
        "infection/infection": "0.20.2",
        "maglnet/composer-require-checker": "2.1.0",
        "mockery/mockery": "1.4.2",
        "phpstan/phpstan": "0.12.64",
        "phpstan/phpstan-strict-rules": "0.12.7",
        "phpunit/phpunit": "9.4.4",
        "rector/rector": "0.8.56",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "platform": {
            "php": "7.4.12"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src",
                "tests/unit",
                "tests/integration"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
