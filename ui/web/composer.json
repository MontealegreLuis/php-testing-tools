{
    "name": "ewallet/web",
    "description": "Ewallet web application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ewallet/application": "1.0",
        "guzzlehttp/psr7": "1.7.0",
        "http-interop/http-factory-guzzle": "1.0.0",
        "monolog/monolog": "2.2.0",
        "psr/container": "1.0.0",
        "psr/http-factory": "1.0.1",
        "psr/http-message": "1.0.1",
        "psr/http-server-handler": "1.0.1",
        "psr/http-server-middleware": "1.0.1",
        "slim/slim": "4.7.1",
        "symfony/dependency-injection": "5.2.1",
        "teapot/status-code": "1.1.2",
        "twig/twig": "3.2.1",
        "vlucas/phpdotenv": "5.2.0"
    },
    "require-dev": {
        "codeception/codeception": "4.1.12",
        "codeception/module-webdriver": "1.1.4",
        "ergebnis/composer-normalize": "2.13.2",
        "friendsofphp/php-cs-fixer": "2.17.2",
        "infection/infection": "0.20.2",
        "maglnet/composer-require-checker": "2.1.0",
        "mockery/mockery": "1.4.2",
        "nelmio/alice": "3.7.4",
        "phpspec/phpspec": "7.0.0",
        "phpspec/prophecy-phpunit": "2.0.1",
        "phpstan/phpstan": "0.12.63",
        "phpstan/phpstan-strict-rules": "0.12.7",
        "phpunit/phpunit": "9.5.0",
        "rector/rector": "0.8.56",
        "roave/security-advisories": "dev-master",
        "symfony/finder": "5.2.1"
    },
    "config": {
        "platform": {
            "php": "7.4.12"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Application\\": "src/",
            "Ewallet\\": "src/",
            "Adapters": "src/",
            "UI\\": "src/",
            "Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
