{
    "name": "ewallet/console",
    "description": "Ewallet console application",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ewallet/application": "~1.0",
        "monolog/monolog": "~1.24",
        "symfony/console": "~4.1",
        "vlucas/phpdotenv": "~2.5"
    },
    "require-dev": {
        "nelmio/alice": "~3.5",
        "phpstan/phpstan": "^0.11.7",
        "phpunit/phpunit": "~7.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ewallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewallet\\": [
                "vendor/ewallet/application/tests/src"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../ewallet"
        }
    ]
}
